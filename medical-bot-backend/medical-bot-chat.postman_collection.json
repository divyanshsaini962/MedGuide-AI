{
  "info": {
    "name": "Medical-Bot-Chat-API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
    "_postman_id": "abcd1234-medical-bot"
  },
  "item": [
    {
      "name": "Ask Medical Bot",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer {{token}}",
            "type": "text"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"question\": \"What is primary aldosteronism and how is it tested?\",\n  \"filter\": { \"source\": { \"$eq\": \"Medical_book .pdf\" } }\n}"
        },
        "url": {
          "raw": "{{base_url}}/api/chat",
          "host": ["{{base_url}}"],
          "path": ["api", "chat"]
        }
      },
      "response": []
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:3000",
      "type": "string"
    },
    {
      "key": "token",
      "value": "",
      "type": "string"
    }
  ]
}
